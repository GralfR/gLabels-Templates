<!DOCTYPE Glabels-templates>
<Glabels-templates>
<!-- =================================================================== -->
<!-- Eigene Label für Druck auf neutralem DinA4                          -->
<!-- =================================================================== -->

  <!-- =================================================================== -->
  <!-- CD-Booklet Jewel Case Front double-sided                        	   -->
  <!-- =================================================================== -->
  <Template size="A4" brand="-noname-" part="CD-Booklet" description="CD Booklet (doublesided) Jewel-Case DinA4">
    <Meta category="label"/>
    <Meta category="media"/>
    <Label-rectangle width="120mm" x_waste="0pt" y_waste="0pt" id="0" height="240mm" round="0pt">
      <Markup-margin size="0pt"/>
      <Markup-line y1="120mm" x1="-3mm" y2="120mm" x2="123mm" />
      <Layout ny="1" x0="45mm" nx="1" y0="28mm" dx="210mm" dy="297mm"/>
    </Label-rectangle>
  </Template>

  <!-- =================================================================== -->
  <!-- CD-Slim-Cover, 140mm x 120mm, 2 per Page                            -->
  <!-- =================================================================== -->
  <Template brand="-noname-" part="CD-Slim" size="A4" description="CD Front-Cover Slim-Case DinA4">
    <Meta category="label"/>
    <Meta category="media"/>
