      <Markup-margin size="3.2mm"/>
      <Markup-line x1="3mm" y1="-5mm" x2="3mm" y2="125mm"/>
      <Markup-line x1="15mm" y1="-5mm" x2="15mm" y2="125mm"/>
      <Markup-line x1="16.5mm" y1="-5mm" x2="16.5mm" y2="125mm"/>
      <Markup-line x1="17.5mm" y1="-5mm" x2="17.5mm" y2="125mm"/>
      <Markup-line x1="19mm" y1="-5mm" x2="19mm" y2="125mm"/>
      <Layout nx="1" ny="2" x0="35mm" y0="18mm" dx="150mm" dy="140mm"/>
    </Label-rectangle>
-->
    <Label-path id="0"  x_waste="0mm" y_waste="0mm" d_units="mm"
      d="M 0 2
        L 15 2 L 15 0 L 140 0 L 140 120 L 15 120 L 15 118 L 0 118 
        Z" >
      <Markup-margin size="3.2mm"/>
      <Markup-line x1="3mm" y1="-5mm" x2="3mm" y2="125mm"/>
      <Markup-line x1="15mm" y1="-5mm" x2="15mm" y2="125mm"/>
      <Markup-line x1="16.5mm" y1="-5mm" x2="16.5mm" y2="125mm"/>
